    "dom-to-image": "^2.6.0",
    "dva": "^2.4.0",
    "dva-loading": "^2.0.5",
    "echarts": "^4.2.0-rc.1",
    "electron": "^4.0.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^13.0.1",
    "enquire-js": "^0.2.1",
    "enzyme": "^3.6.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
