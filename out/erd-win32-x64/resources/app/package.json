{
  "name": "electron-react-dva",
  "version": "1.0.0",
  "description": "electron + react + dva -- rasir",
  "author": "rasir",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=8001 electron . --debug --enable-logging",
    "watch": "cross-env NODE_ENV=development PORT=8001 gulp watch:electron",
    "dev": "cross-env ESLINT=none BROWSER=none PORT=8001 roadhog dev",
    "build": "cross-env ESLINT=none roadhog build",
    "packageDarwin": "electron-packager . 'Hosts' --platform=darwin --arch=x64 --icon=favicon.png --out=./out --asar --app-version=2.0.1 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "packageWin": "electron-packager . erd  --overwrite   --platform=win32 --arch=x64   --out=./out --app-version=2.0.1 ",
    "packageLinux": "electron-packager . 'Hosts' --platform=linux --arch=x64 --out=./out --asar --app-version=2.0.1 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config.js|node_modules)\"",
    "package": "cross-env NODE_ENV=product ESLINT=none && npm run build && npm run packageDarwin && npm run packageWin && npm run packageLinux"
  },
  "license": "ISC",
  "dependencies": {
    "electron-reload": "^1.4.0",
    "iconv-lite": "^0.4.24",
    "nedb": "^1.8.0"
  },
